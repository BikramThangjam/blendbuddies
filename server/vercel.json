{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ],
            "dest": "/"
        },
        { 
            "src": "/(.*)/(.*)",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
              }
            ], 
            "dest": "/"
        }
      ]
  }